{
  "dashboard": {
    "title": "Example API Dashboard",
    "uid": "example-api",
    "timezone": "browser",
    "time": {
      "from": "now-6h",
      "to": "now"
    },
    "refresh": "30s",
    "templating": {
      "list": [
        {
          "type": "query",
          "name": "service",
          "label": "Service",
          "query": "label_values(http_requests_total, service)",
          "datasource": {
            "type": "prometheus",
            "uid": "prometheus-thanos"
          },
          "includeAll": true,
          "multi": true,
          "refresh": 1,
          "sort": 1
        }
      ]
    },
    "panels": [
      {
        "id": 1,
        "type": "stat",
        "title": "QPS",
        "datasource": {
          "type": "prometheus",
          "uid": "prometheus-thanos"
        },
        "targets": [
          {
            "expr": "sum(rate(http_requests_total[1m]))",
            "legendFormat": "",
            "refId": "A"
          }
        ],
        "gridPos": { "h": 3, "w": 4, "x": 0, "y": 0 }
      },
      {
        "id": 2,
        "type": "timeseries",
        "title": "QPS Trend",
        "datasource": {
          "type": "prometheus",
          "uid": "prometheus-thanos"
        },
        "targets": [
          {
            "expr": "sum(rate(http_requests_total[1m]))",
            "legendFormat": "QPS",
            "refId": "A"
          }
        ],
        "gridPos": { "h": 6, "w": 8, "x": 0, "y": 4 }
      }
    ],
    "schemaVersion": 38,
    "version": 1
  },
  "meta": {
    "type": "db",
    "version": 1
  }
}